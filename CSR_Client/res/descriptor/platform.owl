<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
        <!ENTITY spin "http://spinrdf.org/spin#" >
        <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
        <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
        <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
        <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
        <!ENTITY com "http://mmt.inf.tu-dresden.de/documa/ontologies/platform/common#" >
        <!ENTITY dev "http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#" >
        <!ENTITY mod "http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#" >
        <!ENTITY soft "http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#" >
        ]>


<rdf:RDF xmlns="http://mmt.inf.tu-dresden.de/documa/ontologies/platform#"
         xml:base="http://mmt.inf.tu-dresden.de/documa/ontologies/platform"
         xmlns:com="http://mmt.inf.tu-dresden.de/documa/ontologies/platform/common#"
         xmlns:mod="http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#"
         xmlns:dev="http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:spin="http://spinrdf.org/spin#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:soft="http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#">
    <owl:Ontology rdf:about="http://mmt.inf.tu-dresden.de/documa/ontologies/platform">
        <rdfs:comment rdf:datatype="&xsd;string">- 06.10.2014 added device service concept and hasService object
            property
        </rdfs:comment>
        <owl:versionInfo rdf:datatype="&xsd;string">0.8</owl:versionInfo>
        <rdfs:comment rdf:datatype="&xsd;string">02.09.2015 added &apos;hasInterface&apos; datatype property for
            describing the relation between a service entity and its interface represented as URI reference
        </rdfs:comment>
        <owl:versionInfo rdf:datatype="&xsd;string">DPOM Draft Version 2014</owl:versionInfo>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">Oliver Mross</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">The DoCUMA Platform Ontology Model contains concepts to formulate device
            properties and specify application component requirements.
        </rdfs:comment>
    </owl:Ontology>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/common#enablesAspect -->

    <owl:ObjectProperty rdf:about="&com;enablesAspect">
        <rdfs:label rdf:datatype="&xsd;string">enablesAspect</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that something supports or enables a specific
            aspect, e. g., a bidirectional communication. This concept serves as an extension point for other aspect
            concepts
            like interaction task to express, for example, the fact that a device allows a specific interaction form.
        </rdfs:comment>
        <rdfs:range rdf:resource="&com;Aspect"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#currentOperatingSystem -->

    <owl:ObjectProperty rdf:about="&dev;currentOperatingSystem">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">executeOperatingSystem</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the relation that an operating system is executed
            on a specific device. The OS concept encapsulates a specific API, e. g. Cocoa Framework in Mac OS X or
            Android and
            the Android API. This relation helps to identify dependencies between a software and its runtime
            environment.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;execute"/>
        <rdfs:range rdf:resource="&soft;OperatingSystem"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#currentPlatform -->

    <owl:ObjectProperty rdf:about="&dev;currentPlatform">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">executePlatform</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the a platform is executed on a device instance
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;execute"/>
        <rdfs:range rdf:resource="&soft;Platform"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#embeds -->

    <owl:ObjectProperty rdf:about="&dev;embeds">
        <rdfs:label rdf:datatype="&xsd;string">embeds</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the integration of a device as an component from
            another device, i. e., a device could be embeded into another device as an integrated part.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;Device"/>
        <rdfs:domain rdf:resource="&dev;Device"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#embedsBattery -->

    <owl:ObjectProperty rdf:about="&dev;embedsBattery">
        <rdfs:label rdf:datatype="&xsd;string">embedsBattery</rdfs:label>
        <rdfs:comment xml:lang="en">This object propery indicates that a battery is integrated into another device.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;Battery"/>
        <rdfs:domain rdf:resource="&dev;MobileDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;embeds"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#embedsCPU -->

    <owl:ObjectProperty rdf:about="&dev;embedsCPU">
        <rdfs:label rdf:datatype="&xsd;string">embedsCPU</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a CPU is integrated in a computing device, e.
            g., a tablet pc. With the help of this relation it is possible to determine if a single device is an element
            of the
            equivalent class ComputingDevice.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;CPU"/>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;embeds"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#embedsSensor -->

    <owl:ObjectProperty rdf:about="&dev;embedsSensor">
        <rdfs:label rdf:datatype="&xsd;string">embedsSensor</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the integration of sensor individual as part of a
            device.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Device"/>
        <rdfs:range rdf:resource="&dev;Sensor"/>
        <rdfs:subPropertyOf rdf:resource="&dev;embeds"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#execute -->

    <owl:ObjectProperty rdf:about="&dev;execute">
        <rdfs:label rdf:datatype="&xsd;string">execute</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This relation describes the fact that software runs in the context of a
            computing device.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:range rdf:resource="&soft;SoftwareThing"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasAdressWidth -->

    <owl:ObjectProperty rdf:about="&dev;hasAdressWidth">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&dev;AdressWidth"/>
        <rdfs:domain rdf:resource="&dev;CPU"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasAmperage -->

    <owl:ObjectProperty rdf:about="&dev;hasAmperage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasAmperage</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the fact that a battery has a specific amperage
            value.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&com;PhysicalThing"/>
        <rdfs:range rdf:resource="&dev;Amperage"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasFramerate -->

    <owl:ObjectProperty rdf:about="&dev;hasFramerate">
        <rdfs:label rdf:datatype="&xsd;string">hasFramerate</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a camera supports a specific framerate.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;Framerate"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dev;Camera"/>
                    <rdf:Description rdf:about="&dev;Display"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasFreeMemory -->

    <owl:ObjectProperty rdf:about="&dev;hasFreeMemory">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasFreeMemory</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a computing device has a specific amount of
            memory left.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:range rdf:resource="&dev;Memory"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMemory"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasInputDevice -->

    <owl:ObjectProperty rdf:about="&dev;hasInputDevice">
        <rdfs:label rdf:datatype="&xsd;string">hasInputDevice</rdfs:label>
        <rdfs:range rdf:resource="&dev;InputDevice"/>
        <rdfs:domain rdf:resource="&dev;InteractiveDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;embeds"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasMaximalMemory -->

    <owl:ObjectProperty rdf:about="&dev;hasMaximalMemory">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasMaximalMemory</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the fact that a computing device has a maximum
            amount of memory, e. g. a maximum of RAM or cache. This propery allows the determination of current context
            values,
            which are required to determine if a migrating application component can be executed in the context of a
            specific
            target device.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:range rdf:resource="&dev;Memory"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMemory"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasMeasurand -->

    <owl:ObjectProperty rdf:about="&dev;hasMeasurand">
        <rdfs:label rdf:datatype="&xsd;string">hasMeasurand</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the fact that something have a measurand, for
            example, a battery has a a specific amperage and voltage value.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&com;PhysicalThing"/>
        <rdfs:range rdf:resource="&dev;Measurand"/>
        <owl:inverseOf rdf:resource="&dev;isMeasurandFrom"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasMemory -->

    <owl:ObjectProperty rdf:about="&dev;hasMemory">
        <rdfs:label rdf:datatype="&xsd;string">hasMemory</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the fact that a computing device has a specific
            amount of memory.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;ComputingDevice"/>
        <rdfs:range rdf:resource="&dev;Memory"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasOutputDevice -->

    <owl:ObjectProperty rdf:about="&dev;hasOutputDevice">
        <rdfs:label rdf:datatype="&xsd;string">hasOutputDevice</rdfs:label>
        <rdfs:domain rdf:resource="&dev;InteractiveDevice"/>
        <rdfs:range rdf:resource="&dev;OutputDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;embeds"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasPixelDensity -->

    <owl:ObjectProperty rdf:about="&dev;hasPixelDensity">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasPixelDensity</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the fact that a display or a camera have a specific
            pixel density. This relation allows to identify the quality of a display a camera that can be required by a
            software
            component.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Display"/>
        <rdfs:range rdf:resource="&dev;PixelDensity"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasResolution -->

    <owl:ObjectProperty rdf:about="&dev;hasResolution">
        <rdfs:label rdf:datatype="&xsd;string">hasResolution</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a camera or display supports a specific image
            resolution.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;Resolution"/>
        <rdfs:domain rdf:resource="&dev;VisualDevice"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasScale -->

    <owl:ObjectProperty rdf:about="&dev;hasScale">
        <rdfs:label rdf:datatype="&xsd;string">hasScale</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a measurand can have one or more scales. For
            example, a distance can be measured in meter, centermeter or millimeter.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Measurand"/>
        <rdfs:range rdf:resource="&dev;Scale"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasSceneMode -->

    <owl:ObjectProperty rdf:about="&dev;hasSceneMode">
        <rdfs:label rdf:datatype="&xsd;string">hasSceneMode</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a camera can have one or more scene modes, e.
            g., landscape or portrait mode.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Camera"/>
        <rdfs:range rdf:resource="&dev;CameraSceneMode"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasSideRelation -->

    <owl:ObjectProperty rdf:about="&dev;hasSideRelation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasSideRelation</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a image resolution has only a single side
            relation, e. g., the wvga resolution has a side relation of 16 to 9.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Resolution"/>
        <rdfs:range rdf:resource="&dev;SideRelation"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasTemperature -->

    <owl:ObjectProperty rdf:about="&dev;hasTemperature">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasTemperature</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that something has a temperature, e. g., a battery
            or cpu. This relation allows to determine if, for example, a cpu uses much energy resources. This state is a
            critical, regarding the migration of multiple application components onto a specific device with limited
            resources.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&com;PhysicalThing"/>
        <rdfs:range rdf:resource="&dev;Temperature"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#hasVoltage -->

    <owl:ObjectProperty rdf:about="&dev;hasVoltage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasVoltage</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates the fact that a battery has a specific voltage
            value at a particular time.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Battery"/>
        <rdfs:range rdf:resource="&dev;Voltage"/>
        <rdfs:subPropertyOf rdf:resource="&dev;hasMeasurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/device#isMeasurandFrom -->

    <owl:ObjectProperty rdf:about="&dev;isMeasurandFrom">
        <rdfs:label rdf:datatype="&xsd;string">isMeasurandFrom</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a individual is a measurand of something. This
            relation allows to determine which quality is provided by with entity.
        </rdfs:comment>
        <rdfs:range rdf:resource="&com;PhysicalThing"/>
        <rdfs:domain rdf:resource="&dev;Measurand"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#hasDirection -->

    <owl:ObjectProperty rdf:about="&mod;hasDirection">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mod;Modality"/>
        <rdfs:range rdf:resource="&mod;ModalityDirection"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#hasMedium -->

    <owl:ObjectProperty rdf:about="&mod;hasMedium">
        <rdfs:label rdf:datatype="&xsd;string">hasMedium</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a modality has a specific medium (channel) on
            which the information is communicated
        </rdfs:comment>
        <rdfs:range rdf:resource="&mod;Medium"/>
        <rdfs:domain rdf:resource="&mod;Modality"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#hasMode -->

    <owl:ObjectProperty rdf:about="&mod;hasMode">
        <rdfs:label rdf:datatype="&xsd;string">hasMode</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a modality have a specific mode of
            communication that may be within a particular medium. For example, the visual medium, modes include video
            and image,
            because visual information can be entered or displayed continuously (video) or in a discret fashion (image).
        </rdfs:comment>
        <rdfs:domain rdf:resource="&mod;Modality"/>
        <rdfs:range rdf:resource="&mod;Mode"/>
        <owl:inverseOf rdf:resource="&mod;isModeFrom"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#isMediumFrom -->

    <owl:ObjectProperty rdf:about="&mod;isMediumFrom">
        <rdfs:label rdf:datatype="&xsd;string">isMediumFrom</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that medium is used by a specific modality. The
            hasMedium object property is the inverse of this property.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&mod;Medium"/>
        <rdfs:range rdf:resource="&mod;Modality"/>
        <owl:inverseOf rdf:resource="&mod;hasMedium"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#isModeFrom -->

    <owl:ObjectProperty rdf:about="&mod;isModeFrom">
        <rdfs:label rdf:datatype="&xsd;string">isModeFrom</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a mode individual is used by a specific
            modality.
        </rdfs:comment>
        <rdfs:range rdf:resource="&mod;Modality"/>
        <rdfs:domain rdf:resource="&mod;Mode"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#isSupportedByDevice -->

    <owl:ObjectProperty rdf:about="&mod;isSupportedByDevice">
        <rdfs:label rdf:datatype="&xsd;string">isSupportedByDevice</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a modality is supported by a specific device.
            It is the inverse of the hasModality-property.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;Device"/>
        <rdfs:domain rdf:resource="&mod;Modality"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/modality#supportsModality -->

    <owl:ObjectProperty rdf:about="&mod;supportsModality">
        <rdfs:label rdf:datatype="&xsd;string">supportsModality</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a specified device supports a modality. This
            relation allows to determine a which modality is supported by which device.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&dev;Device"/>
        <rdfs:range rdf:resource="&mod;Modality"/>
        <owl:inverseOf rdf:resource="&mod;isSupportedByDevice"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#apiProvidedBy -->

    <owl:ObjectProperty rdf:about="&soft;apiProvidedBy">
        <rdfs:domain rdf:resource="&soft;API"/>
        <rdfs:range rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;providedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#basedOn -->

    <owl:ObjectProperty rdf:about="&soft;basedOn">
        <rdfs:label xml:lang="en">basedOn</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a platform individual executed based on the
            software instance, e. g., a specific operating system or a firmware.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&soft;Platform"/>
        <rdfs:range rdf:resource="&soft;SoftwareThing"/>
        <rdfs:subPropertyOf rdf:resource="&soft;isIncludedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#compilesAndRun -->

    <owl:ObjectProperty rdf:about="&soft;compilesAndRun">
        <rdfs:label rdf:datatype="&xsd;string">compilesAndRun</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a runtime environment is capable of compiling
            and running a specified compiling language, in other words it describes a capability of a runtime
            environment.
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;CompiledLanguage"/>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;run"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#engineProvidedBy -->

    <owl:ObjectProperty rdf:about="&soft;engineProvidedBy">
        <rdfs:label rdf:datatype="&xsd;string">isEngineOf</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a specific software component is an engine
            (includes specific rendering capabilities) of another software component.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&soft;Engine"/>
        <rdfs:range rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;providedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#executedBy -->

    <owl:ObjectProperty rdf:about="&soft;executedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">executedBy</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a software component is executed by a device.
            This relation allows to describe that a software component is currently executed on a specific device. In
            scenarios
            of migrating application components this knowledge is needed to determine the current distribution state of
            all
            distributed application components.
        </rdfs:comment>
        <rdfs:range rdf:resource="&dev;ComputingDevice"/>
        <owl:inverseOf rdf:resource="&dev;execute"/>
        <rdfs:domain rdf:resource="&soft;SoftwareThing"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#formatSupportedBy -->

    <owl:ObjectProperty rdf:about="&soft;formatSupportedBy">
        <rdfs:domain rdf:resource="&soft;Format"/>
        <rdfs:range rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;providedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#hasService -->

    <owl:ObjectProperty rdf:about="&soft;hasService">
        <rdfs:domain rdf:resource="&soft;CSRClient"/>
        <rdfs:range rdf:resource="&soft;Service"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#includes -->

    <owl:ObjectProperty rdf:about="&soft;includes">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="&soft;SoftwareThing"/>
        <rdfs:range rdf:resource="&soft;SoftwareThing"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#interpretAndRun -->

    <owl:ObjectProperty rdf:about="&soft;interpretAndRun">
        <rdfs:label rdf:datatype="&xsd;string">interpretAndRun</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a runtime environment is capable of
            interpreting and running a specified interpretable language, in other words it describes a capability of a
            runtime
            environment.
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;InterpretedLanguage"/>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;run"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#isBrowserEngineOf -->

    <owl:ObjectProperty rdf:about="&soft;isBrowserEngineOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">isBrowserEngineOf</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a engine is part of a browser, in other words
            the engine is used to render webpages in a browser runtime environment.
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;Browser"/>
        <rdfs:domain rdf:resource="&soft;BrowserEngine"/>
        <rdfs:subPropertyOf rdf:resource="&soft;engineProvidedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#isBrowserPluginOf -->

    <owl:ObjectProperty rdf:about="&soft;isBrowserPluginOf">
        <rdfs:label rdf:datatype="&xsd;string">isBrowserPluginOf</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a software component is a plugin of a browser
            runtime environment. This relation allows to formulate dependencies between an application component and
            required
            plugins of the browser-based component runtime environment.
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;Browser"/>
        <rdfs:domain rdf:resource="&soft;Plugin"/>
        <rdfs:subPropertyOf rdf:resource="&soft;pluginProvidedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#isIncludedBy -->

    <owl:ObjectProperty rdf:about="&soft;isIncludedBy">
        <rdfs:domain rdf:resource="&soft;SoftwareThing"/>
        <rdfs:range rdf:resource="&soft;SoftwareThing"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#isPartOfOperatingSystem -->

    <owl:ObjectProperty rdf:about="&soft;isPartOfOperatingSystem">
        <rdfs:label rdf:datatype="&xsd;string">isPartOf</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a software component is integrated or a part
            of a operating system.
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;OperatingSystem"/>
        <rdfs:domain rdf:resource="&soft;SoftwareThing"/>
        <rdfs:subPropertyOf rdf:resource="&soft;isIncludedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#pluginProvidedBy -->

    <owl:ObjectProperty rdf:about="&soft;pluginProvidedBy">
        <rdfs:label rdf:datatype="&xsd;string">isPluginOf</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a software component is a plugin of another
            software component, i. e., it can be seen as an extension component.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&soft;Plugin"/>
        <rdfs:range rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;providedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#providedBy -->

    <owl:ObjectProperty rdf:about="&soft;providedBy">
        <rdfs:range rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:domain rdf:resource="&soft;SoftwareThing"/>
        <rdfs:subPropertyOf rdf:resource="&soft;isIncludedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#provides -->

    <owl:ObjectProperty rdf:about="&soft;provides">
        <rdfs:label xml:lang="en">contains</rdfs:label>
        <rdfs:comment xml:lang="en">This object property represents the relation that describe that a software
            component is integrated as part of an operating system, such as the desktop environment.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:range rdf:resource="&soft;SoftwareThing"/>
        <rdfs:subPropertyOf rdf:resource="&soft;includes"/>
        <owl:inverseOf rdf:resource="&soft;providedBy"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#providesAPI -->

    <owl:ObjectProperty rdf:about="&soft;providesAPI">
        <rdfs:range rdf:resource="&soft;API"/>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;provides"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#providesBrowserEngine -->

    <owl:ObjectProperty rdf:about="&soft;providesBrowserEngine">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&soft;Browser"/>
        <rdfs:range rdf:resource="&soft;BrowserEngine"/>
        <rdfs:subPropertyOf rdf:resource="&soft;providesEngine"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#providesBrowserPlugin -->

    <owl:ObjectProperty rdf:about="&soft;providesBrowserPlugin">
        <rdfs:label rdf:datatype="&xsd;string">executeBrowserPlugin</rdfs:label>
        <rdfs:comment xml:lang="en">This object propery indicates that a software component, e. g., flash runtime
            environment, is a plugin of the browser runtime environment.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&soft;Browser"/>
        <rdfs:range rdf:resource="&soft;BrowserPlugin"/>
        <rdfs:subPropertyOf rdf:resource="&soft;providesPlugin"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#providesEngine -->

    <owl:ObjectProperty rdf:about="&soft;providesEngine">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">hasEngine</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a software component can be based on a
            specific engine. Each engine is linked with a specific set of rendering capabilities, e. g., the capability
            to render
            3D or html-based graphics (lower detail granualarity) or the capability to provide specific operations to
            render a
            data (higher detail granularity), for example, the support of a specific API (HTML5 operations).
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;Engine"/>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;provides"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#providesPlugin -->

    <owl:ObjectProperty rdf:about="&soft;providesPlugin">
        <rdfs:label rdf:datatype="&xsd;string">executePlugin</rdfs:label>
        <rdfs:comment xml:lang="en">This object property represents the relation between a software and another
            software component (plugin) that is integrated as an extension, i. e., the extendable software is still
            executable if
            the extension is not available.
        </rdfs:comment>
        <rdfs:range rdf:resource="&soft;Plugin"/>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;provides"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#run -->

    <owl:ObjectProperty rdf:about="&soft;run">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">runs</rdfs:label>
        <rdfs:comment xml:lang="en">This object property indicates that a runtime environment is capable to execute a
            software component.
        </rdfs:comment>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:range rdf:resource="&soft;SoftwareThing"/>
        <rdfs:subPropertyOf rdf:resource="&soft;includes"/>
    </owl:ObjectProperty>


    <!-- http://mmt.inf.tu-dresden.de/documa/ontologies/platform/software#supportsFormat -->

    <owl:ObjectProperty rdf:about="&soft;supportsFormat">
        <rdfs:range rdf:resource="&soft;Format"/>
        <rdfs:domain rdf:resource="&soft;RuntimeEnvironment"/>
        <rdfs:subPropertyOf rdf:resource="&soft;provides"/>
    </owl:ObjectProperty>


